{
  "name": "foodfast-drone-delivery",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules/.vite dist .vite",
    "validate": "npm run clean && npm install --legacy-peer-deps",
    "predev": "npm run validate",
    "dev": "node scripts/safe-dev.js",
    "dev:fast": "vite",
    "build": "npm run clean && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "postinstall": "rimraf node_modules/.vite dist .vite",
    "reset": "npm run clean && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "axios": "^1.7.2",
    "axios-mock-adapter": "^2.1.0",
    "dayjs": "^1.11.18",
    "framer-motion": "^11.18.2",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "^4.2.1",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "styled-components": "^6.1.19",
    "sweetalert2": "^11.26.3",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^11.0.3",
    "jsdom": "^23.0.0",
    "minimatch": "^10.0.3",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.0.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.4"
  }
}
